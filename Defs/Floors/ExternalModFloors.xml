<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- In case of multiple mods adding the same floor use: MayRequireAnyOf="Mod1.id,Mod2.id,xid.etc"
		 Provided they have the same texturePath -->
	
	<!--  Vanilla Furniture Expanded Architect (MayRequire="vanillaexpanded.vfearchitect")-->
	<!-- Wood -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsWoodPrettyBase" MayRequire="vanillaexpanded.vfearchitect">
		<defName>StuffedWoodFloorVEHerringbone</defName>
		<label>fine herringbone parquet</label>
		<description>Beautifully laid down wooden flooring for that posh, fancy feeling.</description>
		<texturePath>Terrains/Parquet</texturePath>
		<obsoletes>
			<li>VFEArch_WoodenParquet</li>
			<li>VFEArch_WoodenParquetWalnut</li>
			<li>VFEArch_WoodenParquetBirch</li>
			<li>VFEArch_WoodenParquetMahogany</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<!-- Requires Royalty -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsWoodRoyalBase" MayRequire="vanillaexpanded.vfearchitect,ludeon.rimworld.royalty">
		<defName>StuffedWoodFloorVEVersailles</defName>
		<label>versailles parquet</label>
		<description>Beautiful wooden parquet, made with no compromises, for expressing economic status. Slow to construct.</description>
		<texturePath>Terrains/FineWoodenFloor</texturePath>
		<obsoletes>
			<li>VFEArch_FineWoodenFloor</li>
			<li>VFEArch_FineWoodenFloorWalnut</li>
			<li>VFEArch_FineWoodenFloorBirch</li>
			<li>VFEArch_FineWoodenFloorMahogany</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<!-- Stone -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneBasicBase" MayRequire="vanillaexpanded.vfearchitect">
		<defName>StuffedStoneVECobblestone</defName>
		<label>cobblestone floor</label>
		<description>Random-size stone tiles for a rustical, castle feeling. Take less time to construct than ordinary stone tiles and donâ€™t require stone cutting expertise, but lack the beauty of the polished design.</description>
		<texturePath>Terrains/Cobblestone</texturePath>
		<obsoletes>
			<li>VFEArch_CobblestoneFloorSandstone</li>
			<li>VFEArch_CobblestoneFloorGranite</li>
			<li>VFEArch_CobblestoneFloorLimestone</li>
			<li>VFEArch_CobblestoneFloorSlate</li>
			<li>VFEArch_CobblestoneFloorMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStonePrettyBase" MayRequire="vanillaexpanded.vfearchitect">
		<defName>StuffedStoneVETempleSlabs</defName>
		<label>temple slabs</label>
		<description>Large slabs of stone laid at random for that temple-like look. Beautiful to look at.</description>
		<texturePath>Terrains/StoneSlabs</texturePath>
		<obsoletes>
			<li>VFEArch_SlabsSandstone</li>
			<li>VFEArch_SlabsGranite</li>
			<li>VFEArch_SlabsLimestone</li>
			<li>VFEArch_SlabsSlate</li>
			<li>VFEArch_SlabsMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneEpicBase" MayRequire="vanillaexpanded.vfearchitect">
		<defName>StuffedStoneVEPolishedFloor</defName>
		<label>polished stone floor</label>
		<description>Exquisite, polished stone tiles for an elegant, mansion-like look. They are incredibly beautiful to look at.</description>
		<texturePath>Terrains/PolishedStoneFloor</texturePath>
		<obsoletes>
			<li>VFEArch_PolishedStoneFloorSandstone</li>
			<li>VFEArch_PolishedStoneFloorGranite</li>
			<li>VFEArch_PolishedStoneFloorLimestone</li>
			<li>VFEArch_PolishedStoneFloorSlate</li>
			<li>VFEArch_PolishedStoneFloorMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<!-- Metal -->
	<!-- Requires Royalty -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsMetalRoyalBase" MayRequire="vanillaexpanded.vfearchitect,ludeon.rimworld.royalty">
		<defName>StuffedMetalVERoyalTile</defName>
		<label>exquisite tile</label>
		<description>Exquisite metal tiles, made with no compromises, for expressing economic status. Slow to construct.</description>
		<texturePath>Terrains/FineMetalTile</texturePath>
		<obsoletes>
			<li>VFEArch_FineSteelTile</li>
			<li>VFEArch_FineSilverTile</li>
			<li>VFEArch_FineGoldTile</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	
	<!-- Rimefeller (MayRequire="dubwise.rimefeller")-->
	<!-- Stone -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneEpicBase"  MayRequire="dubwise.rimefeller">
		<defName>StuffedStoneDiagonalTilesRF</defName>
		<label>polished diagonal tiles</label>		
		<description>A regal floor made of exquisitly polished mastercrafted stone plates, lined with darker cut stone, for expressing economic status. Slow to construct.</description>
		<texturePath>Rimefeller/Terrain/TilesDiagonal</texturePath>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneRoyalBase"  MayRequire="dubwise.rimefeller">
		<defName>StuffedStoneDiamondTilesRF</defName>
		<label>polished diamond tiles</label>		
		<description>A regal floor made of mastercrafted polished stone plates, with darker cut center stones, for expressing economic status. Slow to construct.</description>
		<texturePath>Rimefeller/Terrain/TilesDiamond</texturePath>
	</StuffedFloors.FloorTypeDef>
	<!-- Wood -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsWoodRoyalBase"  MayRequire="dubwise.rimefeller">
		<defName>StuffedWoodFloorParquetVersaillesRF</defName>
		<label>parquet versailles</label>
		<description>A regal floor made of intricately cut around precisely-measured wooden tiles, made with no compromises, for expressing economic status. Slow to construct.</description>
		<texturePath>Rimefeller/Terrain/Wood_ParquetVersailles</texturePath>
		<obsoletes>
			<li>Wood_ParquetVersailles</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsWoodPrettyBase"  MayRequire="dubwise.rimefeller">
		<defName>StuffedWoodFloorParquetHerringRF</defName>
		<label>parquet herringbone</label>
		<description>A fancy floor made of fine, smoothed, wooden tiles laid in a stylish herringbone pattern.</description>
		<texturePath>Rimefeller/Terrain/Wood_ParquetHerring</texturePath>
		<obsoletes>
			<li>Wood_ParquetHerring</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	
	<!-- Dubs Bad Hygiene (MayRequire="dubwise.dubsbadhygiene")-->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsMetalBasicBase"  MayRequire="dubwise.dubsbadhygiene">
		<defName>StuffedMetalMosaicTileDBH</defName>
		<label>mosaic tile</label>
		<description>Metal tiles in a mosaic pattern, stylish in bathrooms or kitchens.</description>
		<texturePath>DBH/Terrain/Mosaic</texturePath>
		<obsoletes>
			<li>DBHMosaicTile</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneBasicBase"  MayRequire="dubwise.dubsbadhygiene">
		<defName>StuffedStoneMosaicTileDBH</defName>
		<label>mosaic tile</label>
		<description>Carefully-cut and fit stone tiles in a mosaic pattern, stylish in bathrooms or kitchens.</description>
		<texturePath>DBH/Terrain/Mosaic</texturePath>
		<obsoletes>
			<li>DBHMosaicSandstone</li>
			<li>DBHMosaicGranite</li>
			<li>DBHMosaicLimestone</li>
			<li>DBHMosaicSlate</li>
			<li>DBHMosaicMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	
	<!-- VE Memes and Structures (MayRequire="vanillaexpanded.vmemese,ludeon.rimworld.ideology")-->
	<!-- Requires Ideology -->
	
	<!-- Corporate -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBase"  MayRequire="vanillaexpanded.vmemese,ludeon.rimworld.ideology">
		<defName>StuffedStoneCorporateTileVE</defName>
		<label>fine corporate tile</label>
		<description>Fine stone tiles in a corporate style for expressing economic status. Slow to construct.</description>
		<texturePath>Terrain/Surfaces/VME_CorporateFineStoneTiles</texturePath>
		<dominantStyleCategory>VME_Corporate</dominantStyleCategory>
		<obsoletes>
			<li>VME_CorporateFineTileSandstone</li>
			<li>VME_CorporateFineTileGranite</li>
			<li>VME_CorporateFineTileLimestone</li>
			<li>VME_CorporateFineTileSlate</li>
			<li>VME_CorporateFineTileMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	
	<!-- Shintao -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBase"  MayRequire="vanillaexpanded.vmemese,ludeon.rimworld.ideology">
		<defName>StuffedStoneShintaoTileVE</defName>
		<label>shintao fine tile</label>
		<description>Fine stone tiles in a shintao style for expressing economic status. Slow to construct.</description>
		<texturePath>Terrain/Surfaces/VME_TaoistFineTile</texturePath>
		<dominantStyleCategory>VME_Shintaoism</dominantStyleCategory>
		<obsoletes>
			<li>VME_ShintaoFineTileSandstone</li>
			<li>VME_ShintaoFineTileGranite</li>
			<li>VME_ShintaoFineTileLimestone</li>
			<li>VME_ShintaoFineTileSlate</li>
			<li>VME_ShintaoFineTileMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsFabricIdeologyBaseBasic"  MayRequire="vanillaexpanded.vmemese,ludeon.rimworld.ideology">
		<defName>StuffedFabricBasicShintaoCarpetVE</defName>
		<label>shintao carpet</label>
		<description>A nice plush carpet in a Shintaoist style.</description>
		<texturePath>Terrain/Surfaces/VME_TaoistCarpet</texturePath>
		<dominantStyleCategory>VME_Shintaoism</dominantStyleCategory>
		<obsoletes />
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsFabricIdeologyBase"  MayRequire="vanillaexpanded.vmemese,ludeon.rimworld.ideology">
		<defName>StuffedFabricFineShintaoCarpetVE</defName>
		<label>fine shintao carpet</label>
		<description>A beautiful, sublime carpet with a mastercrafted Shintaoist style pattern.</description>
		<texturePath>Terrain/Surfaces/VME_TaoistFineCarpet</texturePath>
		<dominantStyleCategory>VME_Shintaoism</dominantStyleCategory>
		<obsoletes />
	</StuffedFloors.FloorTypeDef>
	
	
	<!-- Alpha Memes (MayRequire="sarg.alphamemes,ludeon.rimworld.ideology")-->
	<!-- Requires Ideology -->
	
	<!-- Jewish -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBasePretty"  MayRequire="sarg.alphamemes,ludeon.rimworld.ideology">
		<defName>StuffedStoneJewishTileAMPretty</defName>
		<label>jewish tile</label>
		<description>Carefully-cut and fit stone tiles in a Jewish style. They are pretty to look at, but they take a long time to lay.</description>
		<texturePath>Terrain/Surfaces/AM_JewishTile</texturePath>
		<dominantStyleCategory>AM_Jewish</dominantStyleCategory>
		<obsoletes>
			<li>AM_JewishTileSandstone</li>
			<li>AM_JewishTileGranite</li>
			<li>AM_JewishTileLimestone</li>
			<li>AM_JewishTileSlate</li>
			<li>AM_JewishTileMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<!-- Fine Jewish Tiles, needs color override -->
	
	<!-- Kemetic -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBasePretty"  MayRequire="sarg.alphamemes,ludeon.rimworld.ideology">
		<defName>StuffedStoneKemeticTileAMPretty</defName>
		<label>kemetic tile</label>
		<description>Carefully-cut and fit stone tiles in a Kemetic style. They are pretty to look at, but they take a long time to lay.</description>
		<texturePath>Terrain/Surfaces/AM_KemeticStoneTiles</texturePath>
		<dominantStyleCategory>AM_Kemetic</dominantStyleCategory>
		<obsoletes>
			<li>AM_KemeticTileSandstone</li>
			<li>AM_KemeticTileGranite</li>
			<li>AM_KemeticTileLimestone</li>
			<li>AM_KemeticTileSlate</li>
			<li>AM_KemeticTileMarble</li>
			<li>AM_KemeticTilePristineLimestone</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBase"  MayRequire="sarg.alphamemes,ludeon.rimworld.ideology">
		<defName>StuffedStoneKemeticTileAM</defName>
		<label>fine kemetic tile</label>
		<description>Fine stone tiles in a Kemetic style for expressing economic status. Slow to construct.</description>
		<texturePath>Terrain/Surfaces/AM_KemeticFineStoneTiles</texturePath>
		<dominantStyleCategory>AM_Kemetic</dominantStyleCategory>
		<obsoletes>
			<li>AM_KemeticFineTileSandstone</li>
			<li>AM_KemeticFineTileGranite</li>
			<li>AM_KemeticFineTileLimestone</li>
			<li>AM_KemeticFineTileSlate</li>
			<li>AM_KemeticFineTileMarble</li>
			<li>AM_KemeticFineTilePristineLimestone</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	
	<!-- Steampunk -->
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneIdeologyBase"  MayRequire="sarg.alphamemes,ludeon.rimworld.ideology">
		<defName>StuffedStoneSteampunkTileAM</defName>
		<label>steampunk tile</label>
		<description>Fine stone tiles in a Steampunk style for expressing economic status. Slow to construct.</description>
		<texturePath>Terrain/Surfaces/AM_SteampunkStoneTiles</texturePath>
		<dominantStyleCategory>AM_SteampunkRevival</dominantStyleCategory>
		<obsoletes>
			<li>AM_SteampunkTileSandstone</li>
			<li>AM_SteampunkTileGranite</li>
			<li>AM_SteampunkTileLimestone</li>
			<li>AM_SteampunkTileSlate</li>
			<li>AM_SteampunkTileMarble</li>
		</obsoletes>
	</StuffedFloors.FloorTypeDef>
	<StuffedFloors.FloorTypeDef ParentName="StuffedFloorsMetalIdeologyBase"  MayRequire="sarg.alphamemes,ludeon.rimworld.ideology">
		<defName>StuffedMetalSteampunkTileAM</defName>
		<label>steampunk tile</label>
		<description>Fine metal tiles in a Steampunk style for expressing economic status. Slow to construct.</description>
		<texturePath>Terrain/Surfaces/AM_SteampunkStoneTiles</texturePath>
